{
  "name": "supabase-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Supabase database operations, authentication, storage, and realtime",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "supabase-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "npm run build && node dist/index.js",
    "inspector": "npm run build && npx @modelcontextprotocol/inspector node dist/index.js"
  },
  "keywords": [
    "mcp",
    "supabase",
    "database",
    "postgres",
    "model-context-protocol"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@supabase/supabase-js": "^2.90.1",
    "dotenv": "^17.2.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  }
}
